#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/.pnpm/npm@10.5.0/node_modules/npm/node_modules/@npmcli/arborist/bin/index.js/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/.pnpm/npm@10.5.0/node_modules/npm/node_modules/@npmcli/arborist/bin/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/.pnpm/npm@10.5.0/node_modules/npm/node_modules/@npmcli/arborist/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/.pnpm/npm@10.5.0/node_modules/npm/node_modules/@npmcli/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/.pnpm/npm@10.5.0/node_modules/npm/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/.pnpm/npm@10.5.0/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules/.pnpm/node_modules:/home/nxtwave/reactjs/coding-practices/nxtwatch/node_modules:/home/nxtwave/reactjs/coding-practices/node_modules:/home/nxtwave/reactjs/node_modules:/home/nxtwave/node_modules:/home/node_modules:/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@npmcli/arborist/bin/index.js" "$@"
else 
  exec node  "$basedir/../@npmcli/arborist/bin/index.js" "$@"
fi
